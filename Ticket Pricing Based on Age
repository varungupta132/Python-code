age = int(input("Enter your age: "))

if age < 5 or age >= 60:
    ticket_price = 0  # Free for children under 5 and seniors 60 and above
elif 5 <= age < 18:
    ticket_price = 10  # Discounted price for children between 5 and 18
else:
    ticket_price = 20  # Standard price for adults

print(f"The ticket price is ${ticket_price}.")
