def first_non_repeating_char(s):
    char_count = {}
    for char in s:
        char_count[char] = char_count.get(char, 0) + 1

    for char in s:
        if char_count[char] == 1:
            return char

    return None

input_str = "leetcode"
result = first_non_repeating_char(input_str)
print(result)
# Output: "l"
